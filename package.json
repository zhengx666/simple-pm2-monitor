    }
  ],
  "bugs": {
    "url": "https://github.com/zhengx666/simple-pm2-monitor/issues"
  },
  "homepage": "https://github.com/zhengx666/simple-pm2-monitor",
  "dependencies": {
    "pm2": "2.6.1",
    "pmx": "1.2.0"
  },
  "config": {
    "log": false,
    "error": true,
    "kill": true,
    "exception": true,
    "restart": true,
